package com.company;
import java.util.Scanner;
public class Main {

    public static void main(String[] args) {

    
        //1.
        Scanner in = new Scanner(System.in);
        System.out.print("Ввести целое число: ");
        int a = in.nextInt();
        System.out.printf("Вывод последний цифры на экран : %d \n", a%10);
        in.close();
        }


}
